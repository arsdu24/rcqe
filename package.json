{
  "name": "@rcqe/nest",
  "version": "0.0.2",
  "author": "arsdu24",
  "license": "ISC",
  "description": "Rabbit CQRS Nest Module",
  "main": "index.js",
  "files": [
    "dist",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"example/**/*.ts\" \"test/**/*.ts\"",
    "start-service1": "ts-node -r dotenv/config -T example/service1/main.ts",
    "start2-service": "ts-node -r dotenv/config -T example/service2/main.ts",
    "lint": "eslint \"{src,example,test}/**/*.ts\" --fix",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/cqrs": "^7.0.0",
    "@nestjs/microservices": "^7.1.0",
    "amqplib": "^0.5.6",
    "amqp-connection-manager": "^3.2.0",
    "reflect-metadata": "^0.1.13"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.0",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/cqrs": "^7.0.0",
    "@nestjs/microservices": "^7.1.0",
    "@types/jest": "25.1.4",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "amqplib": "^0.5.6",
    "amqp-connection-manager": "^3.2.0",
    "reflect-metadata": "^0.1.13",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "directories": {
    "example": "example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arsdu24/rcqe.git"
  },
  "keywords": [
    "rabbit",
    "nest",
    "cqrs",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/arsdu24/rcqe/issues"
  },
  "homepage": "https://github.com/arsdu24/rcqe#readme"
}
